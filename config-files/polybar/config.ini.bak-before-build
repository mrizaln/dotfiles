[module/resource-usage]
type = custom/script
exec = $HOME/.config/polybar/polybar-scripts/resource-usage.sh
tail = false
interval = 3
format-foreground = ${colors.shade7}
format-background = ${colors.shade16}
format-overline = ${colors.background}
format-underline = ${colors.background}
click-left = $HOME/.config/polybar/polybar-scripts/resource-monitor/resource_monitor_py_wrap.sh cpu 20
click-right = $HOME/.config/polybar/polybar-scripts/resource-monitor/resource_monitor_py_wrap.sh mem 20
click-middle = $HOME/.config/polybar/polybar-scripts/resource-monitor/resource_monitor_py_wrap.sh swap 20

label = %output%


[module/vnstat-today]
type = custom/script
exec = $HOME/.config/polybar/polybar-scripts/vnstat-today.sh
tail = false
poll-interval = 60
format-foreground = ${colors.shade7}
format-background = ${colors.shade16}
format-overline = ${colors.background}
format-underline = ${colors.background}
click-right = $HOME/.config/polybar/polybar-scripts/vnstat-daily-usage.sh
click-left = $HOME/.config/rofi/network-manager/nmcli_action.sh
